<?xml version="1.0" encoding="utf-8"?>
<MotionScene
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="4"
        motion:motionInterpolator="cubic(0.78,0.02,0.8,0.8)">
       <KeyFrameSet>
       </KeyFrameSet>
        <OnSwipe />
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:layout_height="58dp"
            android:layout_marginTop="115dp"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_width="145dp"
            android:id="@+id/start"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="400dp"
            motion:layout_constraintVertical_bias="0.0"
            android:rotationX="29"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="145dp"
            android:layout_marginRight="145dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintHorizontal_bias="0.984"
            android:visibility="invisible" />
        <Constraint
            android:layout_marginEnd="150dp"
            android:layout_height="58dp"
            android:layout_marginRight="150dp"
            android:layout_marginTop="364dp"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginBottom="134dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.991"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="145dp"
            motion:layout_constraintVertical_bias="1.0"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="206dp"
            android:layout_marginStart="206dp"
            android:id="@+id/credits"
            android:rotationX="-28"
            motion:transitionEasing="cubic(0.96,0.03,0.8,0.8)"
            android:visibility="invisible" />
        <Constraint
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="1126dp"
            android:layout_height="1028dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:visibility="visible"
            motion:layout_constraintVertical_bias="0.485"
            motion:layout_constraintCircleRadius="@dimen/cardview_compat_inset_shadow"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintHorizontal_bias="0.23"
            motion:layout_constraintStart_toStartOf="parent"
            android:id="@+id/nottronbackground" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/credits"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_height="80dp"
            android:visibility="visible"
            android:rotation="0"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:rotationX="0"
            android:rotationY="0"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.0"
            motion:transitionEasing="decelerate"
            motion:pathMotionArc="startHorizontal"
            motion:layout_constraintVertical_bias="0.277"
            android:layout_width="671dp" />
        <Constraint
            android:id="@+id/start"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_height="74dp"
            android:visibility="visible"
            android:rotation="0"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:rotationX="0"
            android:rotationY="0"
            motion:layout_constraintHorizontal_bias="0.0"
            motion:layout_constraintVertical_bias="1.0"
            motion:transitionEasing="decelerate"
            motion:layout_constraintBottom_toTopOf="@+id/credits"
            android:layout_width="672dp" />
        <Constraint
            android:id="@+id/nottronbackground"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="1126dp"
            android:layout_height="1028dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:visibility="visible"
            motion:layout_constraintVertical_bias="0.219"
            motion:layout_constraintCircleRadius="@dimen/cardview_compat_inset_shadow"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintHorizontal_bias="0.0"
            motion:layout_constraintStart_toStartOf="parent"
            android:rotationX="-360" />
    </ConstraintSet>
</MotionScene>